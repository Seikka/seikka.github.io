                isUC = /(?:UCWEB|UCBrowser)/.test(ua),
                isPc = !isPhone && !isAndroid && !isSymbian;
        return {
          isQB: isQB,
          isTablet: isTablet,
          isPhone: isPhone,
          isAndroid : isAndroid,
          isPc : isPc,
